<?xml version='1.0' encoding='utf-8'?>
<widget id="at.ac.univie.hci.gruppe1.team8" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <name>Restlverwerter</name>
    <description>
        A simple App that searches recipes for certain ingredients.
    </description>
    <author email="fabian.tuerk@gmx.at" href="http://vda.univie.ac.at/Teaching/HCI/15s/schedule.html?">
        Uni Wien, HCI, SoSe 2015, Gruppe1, Team 8
	Natalia Okunska, Werner Lindeis, Martin Treppe, Fabian Tuerk
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
